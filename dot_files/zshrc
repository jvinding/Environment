export DOTNET_CLI_TELEMETRY_OPTOUT=1

hub_sync_prune() {
    local current_branch=$(git_get_branch)
    for b in $(hub sync --color=never 2>&1 | grep --color=never 'was deleted on origin' | cut -d: -f2 | grep --color=never -Eo "^\s*'[^']+'" | grep --color=never -Eo "[^'[:space:]]+" | grep --color=never -Ev '^[[:space:]]*$'); do
        if [ "$current_branch" = "$b" ]; then
            git checkout master || return 1
        fi
        git branch -D "$b"
    done
}

jdk() {
        version=$1
        export JAVA_HOME=$(/usr/libexec/java_home -v"$version");
        java -version
 }

jmeter() {
    jdk 1.8
    ${JMETER_HOME}/bin/jmeter -Jserver.rmi.ssl.disable=true -Jmaven.wagon.http.ssl.insecure=true -Jmaven.wagon.http.ssl.allowall=true -Jmaven.wagon.http.ssl.ignore.validity.dates=true -Jjmeter.loggerpanel.display=true&
}

