## https://hub.docker.com/_/mongo/
mongo_start() {
    docker run -d \
        -p 27017:27017 \
        --name mongo \
        -e MONGO_INITDB_ROOT_USERNAME=admin \
        -e MONGO_INITDB_ROOT_PASSWORD=password \
        mongo
}

mongo_cli() {
    docker exec -it mongo mongo -u admin -p password
}


redis_start() {
    docker run --name redis -p 6379:6379 -d redis
}

httpbin_start(){
    docker run --name httpbin -p 80:4010 -d kennethreitz/httpbin
}

source ~/Environment/plugins/nvm.post_profile
